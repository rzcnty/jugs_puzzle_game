{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Build JugsPuzzle (GCC Linux)",
            "type": "shell",
            "command": "gcc",
            "args": [
                "SpecificToProblem.c",
                "HashTable.c",
                "Standart_Search.c",
                 "GRAPH_SEARCH.c",
                "-o",
                "JugsPuzzle",
                "-g",
                "-Wall",
                "-std=c99"
                "-lm"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "problemMatcher": ["$gcc"]
        }
    ]
}